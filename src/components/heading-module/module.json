{
  "name": "example/heading-module",
  "d4Shortcode": "",
  "title": "Heading Module",
  "titles": "Heading Modules",
  "moduleIcon": "example/module-heading",
  "category": "module",
  "attributes": {
    "module": {
      "type": "object",
      "selector": "{{selector}}",
      "default": {
        "meta": {
          "adminLabel": {
            "desktop": {
              "value": "Heading Module"
            }
          }
        },
        "advanced": {
          "text": {
            "text": {
              "desktop": {
                "value": {
                  "color": "light"
                }
              }
            }
          }
        }
      },
      "defaultPrintedStyle": {
        "decoration": {
          "background": {
            "desktop": {
              "value": {
                "color": "transparent"
              }
            }
          }
        }
      },
      "styleProps": {
        "border": {
          "important": true
        }
      }
    },
    "title": {
      "type": "object",
      "selector": "{{selector}} .example_heading_module__title",
      "default": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "headingLevel": "h2"
                }
              }
            }
          }
        }
      },
      "defaultPrintedStyle": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "size": "26px",
                  "lineHeight": "1em",
                  "weight": "500"
                }
              }
            }
          }
        }
      },
      "styleProps": {
        "font": {
          "important": {
            "font": {
              "desktop": {
                "value": {
                  "color": true
                }
              }
            }
          }
        }
      },
      "tagName": "h2",
      "attributes": {
        "class": "example_heading_module__title"
      },
      "inlineEditor": "plainText",
      "elementType": "heading",
      "childrenSanitizer": "et_core_esc_previously"
    }
  },
  "customCssFields": {
    "contentContainer": {
      "subName": "contentContainer",
      "selectorSuffix": " .example_heading_module__content-container"
    },
    "title": {
      "subName": "title",
      "selectorSuffix": " .example_heading_module__heading"
    }
  }
}
