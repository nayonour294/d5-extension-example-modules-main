{
  "name": "example/advanced-heading",
  "d4Shortcode": "",
  "title": "Advanced Heading",
  "titles": "Advanced Headings",
  "moduleIcon": "example/advanced-heading",
  "category": "module",
  "attributes": {
    "module": {
      "type": "object",
      "selector": "{{selector}}",
      "default": {
        "meta": {
          "adminLabel": {
            "desktop": {
              "value": "Advanced Heading"
            }
          }
        },
        "advanced": {
          "text": {
            "text": {
              "desktop": {
                "value": {
                  "color": "light"
                }
              }
            }
          }
        }
      },
      "defaultPrintedStyle": {
        "decoration": {
          "background": {
            "desktop": {
              "value": {
                "color": "#ecf4f7"
              }
            }
          }
        }
      },
      "styleProps": {
        "border": {
          "important": true
        }
      }
    },
    "container": {
      "type": "object",
      "selector": "{{selector}} .advanced_heading__container",
      "default": {
        "decoration": {
          "background": {
            "desktop": {
              "value": {
                "color": ""
              }
            }
          }
        }
      },
      "attributes": {
        "class": "advanced_heading__container"
      },
      "styleProps": {
        "background": {
          "important": true
        },
        "spacing": {
          "important": true
        }
      }
    },
    "title1": {
      "type": "object",
      "selector": "{{selector}} .advanced_heading__title1",
      "default": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "headingLevel": "h2"
                }
              }
            }
          }
        }
      },
      "defaultPrintedStyle": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "size": "26px",
                  "lineHeight": "1em",
                  "weight": "500"
                }
              }
            }
          }
        }
      },
      "styleProps": {
        "border": {
          "important": true
        },
        "font": {
          "important": {
            "font": {
              "desktop": {
                "value": {
                  "color": true
                }
              }
            }
          }
        }
      },
      "attributes": {
        "class": "advanced_heading__title1",
        "headingLevel": "h2"
       
      },
      "inlineEditor": "plainText",
      "elementType": "heading",
      "childrenSanitizer": "et_core_esc_previously"
    },
    "title2": {
      "type": "object",
      "selector": "{{selector}} .advanced_heading__title2",
      "default": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "headingLevel": "h2"
                }
              }
            }
          }
        }
      },
      "defaultPrintedStyle": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "size": "26px",
                  "lineHeight": "1em",
                  "weight": "500"
                }
              }
            }
          }
        }
      },
      "styleProps": {
        "border": {
          "important": true
        },
        "font": {
          "important": {
            "font": {
              "desktop": {
                "value": {
                  "color": true
                }
              }
            }
          }
        }
      },
      "tagName": "h2",
      "attributes": {
        "class": "advanced_heading__title2"
      },
      "inlineEditor": "plainText",
      "elementType": "heading",
      "childrenSanitizer": "et_core_esc_previously"
    },
    "title3": {
      "type": "object",
      "selector": "{{selector}} .advanced_heading__title3",
      "default": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "headingLevel": "h2"
                }
              }
            }
          }
        }
      },
      "defaultPrintedStyle": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "size": "26px",
                  "lineHeight": "1em",
                  "weight": "500"
                }
              }
            }
          }
        }
      },
      "styleProps": {
        "border": {
          "important": true
        },
        "font": {
          "important": {
            "font": {
              "desktop": {
                "value": {
                  "color": true
                }
              }
            }
          }
        }
      },
      "tagName": "h2",
      "attributes": {
        "class": "advanced_heading__title3"
      },
      "inlineEditor": "plainText",
      "elementType": "heading",
      "childrenSanitizer": "et_core_esc_previously"
    }
  },
  "customCssFields": {
    "contentContainer": {
      "subName": "contentContainer",
      "selectorSuffix": " .advanced-heading__container"
    },
    "title1": {
      "subName": "title1",
      "selectorSuffix": " .advanced_heading__title1"
    },
    "title2": {
      "subName": "title2",
      "selectorSuffix": " .advanced_heading__title2"
    },
    "title3": {
      "subName": "title3",
      "selectorSuffix": " .advanced_heading__title3"
    }
  }
}
