{
  "name": "example/first-module",
  "d4Shortcode": "",
  "title": "First Module",
  "titles": "First Modules",
  "moduleIcon": "example/first-static",
  "category": "module",
  "attributes": {
    "module": {
      "type": "object",
      "selector": "{{selector}}",
      "default": {
        "meta": {
          "adminLabel": {
            "desktop": {
              "value": "First Module"
            }
          }
        },
        "advanced": {
          "text": {
            "text": {
              "desktop": {
                "value": {
                  "color": "light"
                }
              }
            }
          }
        }
      },
      "defaultPrintedStyle": {
        "decoration": {
          "background": {
            "desktop": {
              "value": {
                "color": "#ecf4f7"
              }
            }
          }
        }
      },
      "styleProps": {
        "border": {
          "important": true
        }
      }
    },
    "image": {
      "type": "object",
      "selector": "{{selector}} .example_first_module__image img",
      "styleProps": {
        "border": {
          "important": true
        }
      }
    },
    "title": {
      "type": "object",
      "selector": "{{selector}} .example_static_module__title",
      "default": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "headingLevel": "h2"
                }
              }
            }
          }
        }
      },
      "defaultPrintedStyle": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "size": "26px",
                  "lineHeight": "1em",
                  "weight": "500"
                }
              }
            }
          }

        }
      },
      "styleProps": {
        "font": {
          "important": {
            "font": {
              "desktop": {
                "value": {
                  "color": true
                }
              }
            }
          }
        }
      },
      "tagName": "h2",
      "attributes": {
        "class": "example_static_module__title"
      },
      "inlineEditor": "plainText",
      "elementType": "heading",
      "childrenSanitizer": "et_core_esc_previously"
    },
    "content": {
      "type": "object",
      "selector": "{{selector}} .example_static_module__content",
      "tagName": "div",
      "attributes": {
        "class": "example_static_module__content"
      },
      "inlineEditor": "richText",
      "childrenSanitizer": "et_core_esc_previously",
      "styleProps": {
        "bodyFont": {
          "selectors": {
            "desktop": {
              "value": "{{selector}} .example_static_module__content"
            }
          }
        }
      }
    },
    "button": {
      "type": "object",
      "selector": "{{selector}} .example_first_module__button button",
      "defaultPrintedStyle": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "size": "26px",
                  "lineHeight": "1em",
                  "weight": "600"
                }
              }
            }
          },
          "background": {
            "desktop": {
              "value": {
                "color": "yellow"
              }
            }
          }
        }
      },
      "styleProps": {
        "border": {
          "important": true
        },
        "spacing": {
          "important": true
        },
        "font": {
          "important": {
            "font": {
              "desktop": {
                "value": {
                  "color": true
                }
              }
            }
          }
        }
      },
      "attributes": {
        "class": "example_first_module__button button"
      },
      "tagName": "button",
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously"
    },
    "image2": {
      "type": "object",
      "selector": "{{selector}} .example_first_module__image2, {{selector}} .example_first_module__image2 img",
      "styleProps": {
        "border": {
          "important": true
        }
      }
    },
    "subTitle": {
      "type": "object",
      "selector": "{{selector}} .example_first_module__sub-title",
      "default": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "headingLevel": "h3"
                }
              }
            }
          }
        }
      },
      "defaultPrintedStyle": {
        "decoration": {
          "font": {
            "font": {
              "desktop": {
                "value": {
                  "size": "20px",
                  "lineHeight": "1em",
                  "weight": "500"
                }
              }
            }
          }

        }
      },
      "styleProps": {
        "font": {
          "important": {
            "font": {
              "desktop": {
                "value": {
                  "color": true
                }
              }
            }
          }
        }
      },
      "tagName": "h3",
      "attributes": {
        "class": "example_first_module__sub-title"
      },
      "inlineEditor": "plainText",
      "childrenSanitizer": "et_core_esc_previously"
    }
  },
  "customCssFields": {
    "contentContainer": {
      "subName": "contentContainer",
      "selectorSuffix": " .example_static_module__content-container"
    },
    "title": {
      "subName": "title",
      "selectorSuffix": " .example_static_module__title"
    },
    "content": {
      "subName": "content",
      "selectorSuffix": " .example_static_module__content"
    },
    "image": {
      "subName": "image",
      "selectorSuffix": " .example_first_module__image img"
    },
    "button": {
      "subName": "button",
      "selectorSuffix": " .example_first_module__button button"
    },
    "image2": {
      "subName": "image2",
      "selectorSuffix": " .example_first_module__image2, .example_first_module__image2 img"
    },
    "subTitle": {
      "subName": "subTitle",
      "selectorSuffix": " .example_first_module__sub-title"
    }

  }
}
